{
  "name": "simple-tsx",
  "version": "1.2.2",
  "description": "A simple way to write tsx files. No babel required!",
  "main": "dist/index.js",
  "type": "commonjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha --require ts-node/register tests/**/*.test.ts",
    "build": "tsc -b",
    "lint": "prettier --check . && eslint . --ext .ts",
    "lint-fix": "prettier --write . && eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PythonCoderAS/simple-tsx.git"
  },
  "keywords": [],
  "author": "PythonCoderAS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PythonCoderAS/simple-tsx/issues"
  },
  "homepage": "https://github.com/PythonCoderAS/simple-tsx#readme",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^22.0.0",
    "chai": "^5.0.0",
    "chai-as-promised": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-pythoncoderas-combo": "^1.1.3",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "subprocess-test-utils": "^1.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
